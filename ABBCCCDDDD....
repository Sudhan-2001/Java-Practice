	public static void main(String[] args) {
		System.out.println("Hello World");
			Scanner scan=new Scanner(System.in);
			String d=scan.nextLine();
			char c=d.charAt(0);
		    int count=c- 64;
		    for(int i=0;i<count;i++){
		    for(int j=0;j<count- i;j++){
		        System.out.print(c);
		    }
		    System.out.println();
		    c--;
		}
